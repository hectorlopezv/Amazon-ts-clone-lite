kOut/CheckOut';\n\nimport LoginForm from './containers/Login/LoginForm';\nimport useAuth from './hooks/useAuth';\nexport interface AppProps {\n\n}\n\nconst App: React.FC<AppProps> = () => {\n  const [user] = useAuth();\n  return (\n    <Router>\n      <div className=\"app\">\n        <Switch>\n            <Route exact path=\"/checkout\">\n              <HeaderContainer />\n              <CheckOutContainer />\n              \n            </Route>\n            \n            <Route exact path=\"/login\">\n                <LoginForm subscription={{ submitting: true, pristine: true }} />\n            </Route>\n\n            <Route exact path=\"/\">\n              <HeaderContainer />\n              <HomeContainer />\n            </Route>\n\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n\n","/home/hvmlopez/amazonclone_ts_redux/src/globalStyles.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/containers/Header/Header.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/Header/index.tsx",["145"],"import React from 'react';\nimport {\n    Container, \n    Search as HSearch, \n    Frame,\n    Input as Hinput,\n    Nav as Hnav,\n    Option as Hoption,\n    Option1,\n    Option2,\n    Logo as Hlogo,\n    SearchIco as HsearchIcon,\n    BasketIcon,\n    Basket as Hbasket,\n    BasketText,\n    Link as Hlink\n} from './styles/Header';\nimport { RouteProps} from \"react-router-dom\";\nimport {useSelector} from 'react-redux';\n\n\nexport interface LinkProps {\n    to: string;\n}\n \nconst Link: React.FC<LinkProps &  RouteProps>  = ({children, to, ...props}) => {\n    return (  <Hlink to={to} {...props}>{children}</Hlink> );\n}\n \n\nexport interface SearchProps {\n    \n}\n \nconst Search: React.FC<SearchProps> = ({children, ...props}) => {\n    return ( <HSearch {...props}>{children} </HSearch> );\n}\n\n\nexport interface NavProps {\n    \n}\n \nconst Nav: React.FC<NavProps> = ({children, ...props}) => {\n    return ( <Hnav {...props}>{children} </Hnav> );\n}\n\nexport interface OptionProps {\n    onClick?: any;\n}\n \nconst Option: React.FC<OptionProps> = ({children, ...props}) => {\n    return ( <Hoption {...props}>{children} </Hoption> );\n}\n\nexport interface InputProps {\n    \n}\n \nconst Input: React.FC<InputProps> = ({...props}) => {\n    return ( <Hinput {...props} /> );\n}\n \nexport interface SpanProps {\n    \n\n}\n \nconst SpanOne: React.FC<SpanProps> = ({children, ...props}) => {\n    return ( \n      <Option1 {...props}>{children}</Option1> \n    );\n}\n\nconst SpanTwo: React.FC<SpanProps> = ({children, ...props}) => {\n    return ( \n      <Option2 {...props}>{children}</Option2> \n    );\n}\n\nexport interface LogoProps {\n    src: string;\n    alt: string;\n}\n \nconst Logo: React.FC<LogoProps> = ({...props}) => {\n    return (  \n        <Hlogo {...props}/>\n    );\n}\n\nexport interface SearchIconProps {\n    \n}\n \nconst SearchIcon: React.FC<SearchIconProps> = () => {\n    return (  \n        <HsearchIcon />\n    );\n}\n \nexport interface BasketProps {\n    \n}\n \nconst Basket: React.FC<BasketProps> = ({children, ...props}) => {\n    const lenght = useSelector((stateCurrent: any) => stateCurrent.App.quantityItems);\n   \n    return ( \n        <Hbasket {...props}>\n            <BasketIcon />\n            <BasketText>{lenght}</BasketText>\n        </Hbasket> \n      );\n}\n\n////////////////////////////////////////////////////////////////////////////////////////////////\n\nexport interface HeaderProps {\n}\n\ninterface CompoundComponets {\n    Search: React.FC<SearchProps>;\n    Nav: React.FC<NavProps>;\n    Option: React.FC<OptionProps>;\n    Input: React.FC<InputProps>;\n    SpanOne: React.FC<SpanProps>;\n    SpanTwo: React.FC<SpanProps>;\n    Logo: React.FC<LogoProps>;\n    SearchIcon: React.FC<SearchIconProps>;\n    Basket: React.FC<BasketProps>;\n    Link: React.FC<LinkProps>;\n}\n\nconst Header: React.FC<HeaderProps> & CompoundComponets = ({children, ...props}) => {\n    return (<Container {...props}>{children} </Container>  );\n}\n \n\n\n//styled components\nHeader.Search = Search;\nHeader.Nav = Nav;\nHeader.Option = Option;\nHeader.Input = Input;\nHeader.SpanOne = SpanOne;\nHeader.SpanTwo = SpanTwo;\nHeader.Logo = Logo;\nHeader.SearchIcon = SearchIcon;\nHeader.Basket = Basket;\nHeader.Link = Link;\nexport default Header;","/home/hvmlopez/amazonclone_ts_redux/src/components/Header/styles/Header.tsx",["146"],"import styled from 'styled-components/macro';\nimport {Link as Link_, RouteProps} from \"react-router-dom\";\nimport SearchIcon from '@material-ui/icons/Search';\nimport AddShoppingCartIcon from '@material-ui/icons/AddShoppingCart';\n\nexport const Container = styled.div`\n    height: 60px;\n    display: flex;\n    align-items: center;\n    background-color: #131921;\n    position: sticky;\n    top: 0;\n    z-index: 100;\n`;\n\nexport const Search = styled.div`\n    display: flex;\n    flex: 1;\n    align-items: center;\n    border-radius: 24px;\n`;\n\nexport const Link = styled(Link_)`\n    \n\n`;\n\nexport const SearchIco = styled(SearchIcon)`\n    padding: 1.5px;\n    background-color: #cd9042;\n    font-size: 20px !important;\n`;\n\n\nexport const Logo = styled.img`\n    width: 100px;\n    object-fit: contain;\n    margin: 0 20px;\n    margin-top: 18px;\n`;\n\nexport const Input = styled.input`\n    height: 12px;\n    padding: 10px;\n    border: none;\n    width: 100%;\n`;\n\nexport const Frame = styled.div`\n\n`;\n\n\nexport const Nav = styled.nav`\n    display: flex;\n    justify-content: space-between;\n`;\n\nexport const Option = styled.div`\n    display: flex;\n    flex-direction: column;\n    margin-left: 10px;\n    margin-right: 10px;\n    color: white;\n`;\n\n\nexport const Option1 = styled.span`\n    font-size: 10px;\n`;\n\nexport const Option2 = styled.span`\n    font-size: 13px;\n    font-weight: 800;\n`;\n\nexport const Basket = styled.div`\n    display: flex;\n    align-items: center;\n    color: white;\n`;\n\n//extends Option1\nexport const BasketText = styled(Option1)`\n    margin-left: 10px;\n    margin-right: 10px;\n`;\n\nexport const BasketIcon = styled(AddShoppingCartIcon)`\n    padding: 1.5px;\n    font-size: 24px !important\n`;\n\n\n\n","/home/hvmlopez/amazonclone_ts_redux/src/containers/Home/Home.tsx",["147","148"],"import React from 'react'\nimport Home from '../../components/Home';\nimport ProductContainer from '../../containers/Product/Product';\nimport { useSpring, animated } from 'react-spring';\nimport {products_1,products_2, products_3} from '../../fixtures/Product';\n\nexport interface HomeContainerProps {\n    \n}\n\n\nconst HomeContainer: React.FC<HomeContainerProps> = () => {\n    //add slider to images\n    return (  \n        <Home>\n            <Home.Frame>\n                <Home.Image  \n                    src=\"https://images-eu.ssl-images-amazon.com/images/G/02/digital/video/merch2016/Hero/Covid19/Generic/GWBleedingHero_ENG_COVIDUPDATE__XSite_1500x600_PV_en-GB._CB428684220_.jpg\"\n                    alt=\"Background Image\"\n                />\n                <Home.Row>\n                    {products_1.map((item: any) => (\n                        <ProductContainer\n                            key={item.id}\n                            id={item.id} \n                            title={item.title}\n                            image={item.image}\n                            price={item.price}\n                            rating={item.rating}\n                        />\n                    ))}\n                </Home.Row>\n                <Home.Row>\n                    {products_2.map((item: any) => (\n                        <ProductContainer\n                        key={item.id}\n                            id={item.id} \n                            title={item.title}\n                            image={item.image}\n                            price={item.price}\n                            rating={item.rating}\n                        />\n                    ))}\n                </Home.Row>\n\n                <Home.Row>\n                    {products_3.map((item: any) => (\n                        <ProductContainer\n                        key={item.id}\n                            id={item.id} \n                            title={item.title}\n                            image={item.image}\n                            price={item.price}\n                            rating={item.rating}\n                        />\n                    ))}\n                </Home.Row>\n\n\n            </Home.Frame>\n        </Home>\n    );\n}\n \nexport default HomeContainer;","/home/hvmlopez/amazonclone_ts_redux/src/components/Home/index.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/Home/styles/Home.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/containers/Product/Product.tsx",["149"],"import React from 'react';\nimport Product from '../../components/Product';\nimport {useSelector, useDispatch} from 'react-redux';\nimport {addBasket} from '../../store/Actions/App';\n\n\nexport interface ProductContainerProps {\n    title: string;\n    image: string;\n    price: string;\n    rating: number;\n    id: string;\n}\n \nconst ProductContainer: React.FC<ProductContainerProps> = ({id, title, image, price, rating}) => {\n    //redux dispathc action\n    const dispatch = useDispatch();\n    const basket =  useSelector((stateCurrent: any) => stateCurrent.App.basket);\n    const AddToBasket = (id: string, title: string, image: string, price: string, rating: number) => dispatch(addBasket(id, title, image, price, rating));\n    return (  \n        <Product>\n            <Product.Info>\n                <Product.Text>{title}</Product.Text>\n                <Product.Price>\n                    <small>$</small> \n                    <strong>{price}</strong>\n                </Product.Price>\n                <Product.Rating>\n                    {Array(rating).fill(rating).map((_,i) => (\n                       <p key={i}>🌟</p> \n                    ))}\n                    \n                </Product.Rating> \n            </Product.Info>\n            <Product.Image \n                src={image} \n                alt=\"Product Image\"\n            />\n            <Product.Button onClick={() => AddToBasket(id, title, image, price, rating,)}>Add To Basket</Product.Button>\n        </Product>\n    );\n}\n \nexport default ProductContainer;","/home/hvmlopez/amazonclone_ts_redux/src/components/Product/index.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/Product/styles/Product.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/CheckOut/index.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/CheckOut/styles/CheckOut.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/SubTotal/index.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/SubTotal/styles/SubTotal.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/containers/SubTotal/SubTotal.js",[],["150","151"],"/home/hvmlopez/amazonclone_ts_redux/src/store/Reducers/App.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/store/Actions/ActionTypes.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/store/Actions/App.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/CheckOut/CheckOutProduct/index.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/CheckOut/CheckOutProduct/styles/CheckOutProduct.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/containers/Login/LoginForm.tsx",["152","153"],"import React from 'react';\nimport Login from '../../components/Login';\nimport {Link} from 'react-router-dom';\nimport { Form, Field, FormSpy } from 'react-final-form';\nimport {db, auth} from '../../lib/firebase.prod';\nimport {useHistory} from 'react-router-dom';\n\n\n\nexport interface LoginFormProps {\n    subscription: { submitting: boolean, pristine: boolean } \n}\n \nconst LoginForm: React.FC<LoginFormProps> = ({children, subscription, ...props}) => {\n    const history = useHistory();\n    const required = (value: any) => (value ? undefined : 'Required');\n\n    const onSubmit = async (values: any) => {\n        //Sigin IN using authentication and push to Home page\n        //and push to the Store! and LocalStorage\n        auth.signInWithEmailAndPassword(values.Email, values.Password)\n        .then((auth) => {\n            history.push('/');\n        })\n        .catch((error) => {\n            console.log(error);\n        })\n    }\n\n    const onCreateAccount = (values: any) => {\n        //create Account\n        auth.createUserWithEmailAndPassword(values.Email, values.Password)\n        .then((auth: any)=> {\n            auth.user.updateProfile({\n                displayName: values.Email,\n            });\n        })\n        .then(() => {\n            //put it the redux store and Local Storage\n                history.push('/');\n        })\n        .catch(error => {\n            //put Modal for the error\n            console.log(error); \n        })\n    }\n\n    return (  \n    <Login>\n        <Link to=\"/\">\n            <Login.Image  src='https://upload.wikimedia.org/wikipedia/commons/thumb/a/a9/Amazon_logo.svg/1024px-Amazon_logo.svg.png' alt=\"image\"/>\n        </Link>\n        <Login.Frame>\n            <Login.Title>Sign In</Login.Title>\n\n            <Form\n                onSubmit={onSubmit}\n                subscription={subscription}\n                render={({ handleSubmit, form, submitting, pristine, values }) => (\n                <form onSubmit={handleSubmit}>\n                    <Field name=\"Email\" validate={required} render={(props: any)=> (\n                    <div>\n                        <Login.Label>E-mail</Login.Label>\n                        <Login.Input type=\"text\" {...props.input}/> \n                    </div>)}/>\n                    <Field name=\"Password\" validate={required} render={(props: any)=> (\n                    <div>\n                        <Login.Label>Password</Login.Label>\n                        <Login.Input type=\"password\" {...props.input}/> \n                    </div>)} />\n                    <Login.Button type=\"submit\" disabled={submitting || pristine}>\n                        Sign In\n                    </Login.Button>\n                    <Login.Text>\n                        By signing-in you agree to the AMAZON UI CLONE  Conditions of Use & Sale. Please\n                        see our Privacy Notice, our Cookies Notice and our Interest-Based Ads Notice.\n                    </Login.Text>\n\n                    <Login.Button type=\"button\" disabled={submitting || pristine} onClick={() => onCreateAccount(form.getState().values)}>\n                        Create your Amazon Account\n                    </Login.Button>\n                </form>\n                )}\n            />\n\n        </Login.Frame>\n    </Login>);\n}\n \nexport default LoginForm;\n","/home/hvmlopez/amazonclone_ts_redux/src/components/Login/index.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/components/Login/styles/Login.tsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/lib/firebase.prod.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/hooks/useAuth.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/store/Actions/User.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/store/Reducers/User.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/fixtures/Product.ts",[],"/home/hvmlopez/amazonclone_ts_redux/src/containers/CheckOut/CheckOut.jsx",[],"/home/hvmlopez/amazonclone_ts_redux/src/containers/CheckOut/CheckOutProduct/CheckOutProduct.jsx",["154"],{"ruleId":"155","replacedBy":"156"},{"ruleId":"157","replacedBy":"158"},{"ruleId":"159","severity":1,"message":"160","line":15,"column":10,"nodeType":"161","messageId":"162","endLine":15,"endColumn":14},{"ruleId":"159","severity":1,"message":"163","line":5,"column":5,"nodeType":"161","messageId":"162","endLine":5,"endColumn":10},{"ruleId":"159","severity":1,"message":"164","line":2,"column":24,"nodeType":"161","messageId":"162","endLine":2,"endColum