ce>\n                <Product.Rating>\n                    {Array(rating).fill(rating).map((_,i) => (\n                       <p key={i}>🌟</p> \n                    ))}\n                    \n                </Product.Rating> \n            </Product.Info>\n            <Product.Image \n                src={image} \n                alt=\"Product Image\"\n            />\n            <Product.Button onClick={() => AddToBasket(id, title, image, price, rating,)}>Add To Basket</Product.Button>\n        </Product>\n    );\n}\n \nexport default ProductContainer;","/home/hector/Amazon-ts-clone-lite/src/containers/CheckOut/CheckOutProduct/CheckOutProduct.jsx",["197"],"import React from 'react';\nimport CheckOutProduct from '../../../components/CheckOut/CheckOutProduct';\n\nclass CheckOutProductContainer extends React.Component {\n    \n    render(){\n        const {onClick, quantity,id, title, image, price, rating} = this.props; \n        return (\n\n            <CheckOutProduct ref={this.ref}>\n            <CheckOutProduct.Image src={image} alt=\"\" />\n            \n            <CheckOutProduct.Info>\n                <CheckOutProduct.Title>{title+ ` (${quantity})`}</CheckOutProduct.Title>\n                <CheckOutProduct.Price><small>$</small><strong>{price}</strong></CheckOutProduct.Price>\n                <CheckOutProduct.Rating>\n                    {Array(rating).fill(rating).map((_,i) => (\n                       <p key={i}>🌟</p> \n                    ))}\n                </CheckOutProduct.Rating>\n                <CheckOutProduct.Button onClick={onClick}>Remove from basket</CheckOutProduct.Button>\n            </CheckOutProduct.Info>\n            \n        </CheckOutProduct>\n        );\n    }\n}\n\nexport default CheckOutProductContainer;","/home/hector/Amazon-ts-clone-lite/src/components/Header/index.tsx",["198"],"import React from 'react';\nimport {\n    Container, \n    Search as HSearch, \n    Frame,\n    Input as Hinput,\n    Nav as Hnav,\n    Option as Hoption,